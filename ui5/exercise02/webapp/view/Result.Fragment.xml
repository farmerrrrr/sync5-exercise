<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog
        id="resultDialog"
        title="연산 결과">
        <content>
            <Text 
                text="{/result}"
                class="sapUiMediumMargin"/>
        </content>
        <beginButton>
            <Button 
                text="닫기"
                press=".onCloseResultDialog"/>
        </beginButton>
    </Dialog>

</core:FragmentDefinition>